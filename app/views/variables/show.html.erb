<div class="page-header">
  <h3>Variable #<%= @variable.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Indicator</dt>
      <dd><%= @variable.indicator_id %></dd>

      <dt>Model</dt>
      <dd><%= @variable.model_id %></dd>

      <dt>Weight</dt>
      <dd><%= @variable.weight %></dd>

      <dt>Ourmodel</dt>
      <dd><%= @variable.ourmodel_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/variables" class="btn btn-primary">
        Back
      </a>
      <a href="/variables/<%= @variable.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_variable/<%= @variable.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
