<div class="page-header">
  <h1>
    Ourmodels
    <a href="/ourmodels/new" class="btn btn-lg btn-success">New Ourmodel</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Model weight</th>
        <th>Ourmodel name</th>
        <th>Actions</th>
      </tr>

      <% @ourmodels.each do |ourmodel| %>
      <tr>
        <td><%= ourmodel.model_weight %></td>
        <td><%= ourmodel.ourmodel_name %></td>
        <td>
          <a href="/ourmodels/<%= ourmodel.id %>" class="btn btn-primary">Show</a>
          <a href="/ourmodels/<%= ourmodel.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_ourmodel/<%= ourmodel.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
